{
    "name": "vscode-airapps-ial",
    "displayName": "iAL",
    "description": "This extension will add some intelligence to AL. Just type the name of you action, and iAL will find best image for your button among 1400+ standard images using machine learning algorithms.",
    "version": "0.0.1",
    "publisher": "AirApps",
    "engines": {
        "vscode": "^1.19.0"
    },
    "author": {
        "email": "dmitry.katson@gmail.com",
        "name": "Dmitry Katson"
    },
    "categories": [
        "Extension Packs","Other"
    ],
    "activationEvents": [
        "onCommand:extension.getimage",
        "onLanguage:al"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.getimage",
                "title": "iAL: Get image for action"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
