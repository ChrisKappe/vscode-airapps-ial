{
    "name": "vscode-airapps-ial",
    "displayName": "iAL",
    "description": "This extension will add some intelligence to AL. Just type the name of your action, and iAL will find the best image for it, among 1400+ standard images using machine learning algorithms.",
    "version": "0.1.0",
    "publisher": "dkatson",
    "engines": {
        "vscode": "^1.19.0"
    },
    "author": {
        "email": "dmitry.katson@gmail.com",
        "name": "Dmitry Katson"
    },
    "icon": "/src/images/iAL_logo.png",
    "categories": [
        "Extension Packs",
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dkatson/vscode-airapps-ial"
    },
    "activationEvents": [
        "onCommand:extension.getimage",
        "onLanguage:al"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.getimage",
                "title": "iAL: Get image for action"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.1.10",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "opn": "^4.0.2",
        "request": "^2.83.0",
        "vsce": "^1.35.0"
    }
}
